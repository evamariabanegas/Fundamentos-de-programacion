#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> v = {1, 2, 3, 4, 5};
    vector<int> r(v.rbegin(), v.rend());
    cout << "Vector original: ";
    for (int x : v) cout << x << " ";
    cout << "\nVector invertido: ";
    for (int x : r) cout << x << " ";
    cout << endl;
    return 0;
}
