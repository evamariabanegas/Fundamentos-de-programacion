#include <iostream>
#include <vector>
#include <unordered_set>
using namespace std;

int main() {
    vector<int> v = {1, 2, 3, 2, 4, 3, 5, 1};
    unordered_set<int> vistos;
    vector<int> sinDuplicados;

    for (int x : v) {
        if (!vistos.count(x)) {
            sinDuplicados.push_back(x);
            vistos.insert(x);
        }
    }

    cout << "Vector sin duplicados: ";
    for (int x : sinDuplicados) cout << x << " ";
    return 0;
}
