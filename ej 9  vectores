#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> A = {1, 2, 3};
    vector<int> B = {4, 5, 6, 7};
    vector<int> C = A;
    C.insert(C.end(), B.begin(), B.end());

    cout << "Vector concatenado: ";
    for (int x : C) cout << x << " ";
    cout << endl;
    return 0;
}
