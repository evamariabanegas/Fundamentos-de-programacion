#include <stdio.h>

int main() {
    int n;
    printf("Ingrese n (>=0): ");
    if (scanf("%d", &n) != 1) return 1;
    if (n < 0) { printf("n debe ser >=0\n"); return 0; }

    unsigned long long a = 1, b = 1;
    if (n == 0) { printf("F0 = 1\n"); return 0; }
    if (n == 1) { printf("F1 = 1\n"); return 0; }

    for (int i = 2; i <= n; ++i) {
        unsigned long long c = a + b;
        a = b;
        b = c;
    }
    printf("F%d = %llu\n", n, b);
    return 0;
}
