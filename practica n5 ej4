#include <stdio.h>

int main() {
    int h1, m1, s1, h2, m2, s2;
    printf("Ingrese primer horario (HH MM SS): ");
    if (scanf("%d %d %d", &h1, &m1, &s1) != 3) return 1;
    printf("Ingrese segundo horario (HH MM SS): ");
    if (scanf("%d %d %d", &h2, &m2, &s2) != 3) return 1;

    int t1 = h1*3600 + m1*60 + s1;
    int t2 = h2*3600 + m2*60 + s2;

    if (t2 < t1) {
        printf("Error: el primer horario debe ser antes que el segundo (mismo dia).\n");
        return 0;
    }

    int diff = t2 - t1;
    int hd = diff / 3600;
    diff %= 3600;
    int md = diff / 60;
    int sd = diff % 60;

    printf("Tiempo transcurrido: %d:%02d:%02d\n", hd, md, sd);
    return 0;
