#include <iostream>
#include <vector>
using namespace std;
void minimo(const vector<float> &vec) {
    if (vec.size()) {
        cout << "El vector está vacío." << endl;
        return;
    }
    float minVal = vec[0];
    for (int i = 1; i < vec.size(); i++) {
        if (vec[i] < minVal) {
            minVal = vec[i];
        }
    }
    cout << "El vector minimo: " << minVal << endl;
}
int main() {
    int n;
    cout << "Cuantos valores deseas ingresar? ";
    cin >> n;
    vector<float> valores(n);
    cout << "Ingresa los valores (pueden ser enteros o reales):" << endl;
    for (int i = 0; i < n; i++) {
        cin >> valores[i];
    }
    minimo(valores);
    return 0;
}
