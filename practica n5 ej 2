#include <stdio.h>
#include <math.h>
 int main () {

     double a,b,c,x1,x2;

     printf("Ingrese el termino cuadratico:");
     scanf("%lf,",&a);
     printf("Ingrese el termino lineal:");
     scanf("%lf",&b);
     printf("Ingrese el termino independiente:");
     scanf("%lf",&c);

     if (b*b-4*a*c >= 0) {
        x1=(-b+sqrt(b*b-4*a*c))/(2*a);
        x2=(-b-sqrt(b*b-4*a*c))/(2*a);
        printf("Las soluciones son x1=%lf y x2=%lf\n",x1,x2);
     }
        else {
                printf("La ecuacion no tiene soluciones reales.\n");

        }
        return 0;
 }
